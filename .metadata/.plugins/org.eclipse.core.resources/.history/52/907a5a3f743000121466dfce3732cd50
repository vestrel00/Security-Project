package com.vestrel00.ssc.server.shared;

/**
 * Simple settings mainly used for debugging.
 * 
 * @author Estrellado, Vandolf
 * 
 */
public class SSCSettings {

	public boolean showProtocolsAtWork;

	public SSCSettings(boolean showProtocolsAtWork) {
		this.showProtocolsAtWork = showProtocolsAtWork;
	}

}
